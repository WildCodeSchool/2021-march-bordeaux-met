{% extends 'layout.html.twig' %}

{% block content %}
    <h1>Users</h1>
    <ul>
        {%  for user in users %}
            <li>
                <a href="/admin/show/{{ user.id }}"> {{ user.id }}. {{ user.pseudo }}</a>
            </li>
        {% else %}  
            <li>
                <p>Nothing to display</p>
            </li>
        {% endfor %}
    </ul>
    <a href="/admin/add" > Add</a>
{% endblock %}

